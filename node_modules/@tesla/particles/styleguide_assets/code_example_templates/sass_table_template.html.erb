<div class="code-example">
  <div class="code-table">
    <table>
      <tbody>
        <% examples.each do |example| %>
          <tr>
            <th>
              <div class="code-example">
                <%= example.rendered_example %>
              </div>
            </th>
            <td>
              <div class="code-block code-block--sass">
                <div class="highlight">
                  <pre><code class="scss"><%= example.code_example %></code></pre>
                </div>
              </div>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
